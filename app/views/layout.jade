doctype html
html
  head
    title
      block title
        = title
        = " | " + (siteSettings.name || "Members Area")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    //link(rel='stylesheet', href='/css/bootstrap.min.css')
    //link(rel='stylesheet', href='/css/bootstrap-theme.min.css')
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css')
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css')
    link(rel='stylesheet', href='/css/style.css')
    link(rel='stylesheet', href='//cdnjs.cloudflare.com/ajax/libs/metisMenu/2.5.0/metisMenu.min.css')
    link(href="/img/favicon.png", rel="shortcut icon")
  body
    #wrapper.container
      include ./alert.jade
      include ./navigation.jade
      block everything
        #main.container
          block body
            .row
              .col-md-12
                block content
      include ./footer.jade

    //script(src='/js/jquery.min.js')
    //script(src='/js/jquery-migrate.min.js')
    script(src='/js/jquery-2.2.3.min.js')
    script(src='/js/jquery-migrate-1.3.0.min.js')
    //script(src='/js/bootstrap.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js')
    script(src='//cdnjs.cloudflare.com/ajax/libs/metisMenu/2.5.0/metisMenu.min.js')
    script(src='/js/members.js')
